<div class="container pt-4">
    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h2>Balance General</h2>
                </div>
            </div>
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th>transacion</th>
                        <th>Cantidad</th>
                        <th>transacion</th>
                        <th>Cantidad</th>
                        <th>transacion</th>
                        <th>Cantidad</th>
                        <th>transacion</th>
                        <th>Cantidad</th>
                        <th>transacion</th>
                        <th>Cantidad</th>
                        <th>transacion</th>
                        <th>Cantidad</th>
                        <th>Total</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each data}}
                        <tr>
                            <td>{{nombreUno}}</td>
                            <td>{{cantidadUno}}</td>
                            <td>{{nombreDos}}</td>
                            <td>{{cantidadDos}}</td>
                            <td>{{nombreTres}}</td>
                            <td>{{cantidadTres}}</td>
                            <td>{{nombreCuatro}}</td>
                            <td>{{cantidadCuatro}}</td>
                            <td>{{nombreCinco}}</td>
                            <td>{{cantidadCinco}}</td>
                            <td>{{nombreSeis}}</td>
                            <td>{{cantidadSeis}}</td>
                            <td>{{result}}</td>
                        </tr>
                    {{else}}
                        <tr>
                            <p>No hay datos registrados!</p>
                        </tr>


                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>